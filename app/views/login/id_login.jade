h1 login
form.form.col-md-12.center-block(name='form')
  .form-group
   input.form-control.input-lg(type='name', placeholder="your id", ng-model="userInfo.id",requered)
   .form-group
    input.form-control.input-lg(type='password', placeholder='Password', ng-model="userInfo.password",requered)
  .form-group
    button.btn.btn-primary.btn-lg.btn-block(ng-click='signIn()',ng-disabled="form.$invalid") Sign In

